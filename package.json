{
  "name": "openfin-react-concise",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chalk": "2.4.2",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "express": "^4.16.4",
    "hadouken-js-adapter": "^0.39.1",
    "morgan": "^1.9.0",
    "rimraf": "^2.6.2",
    "tcp-port-used": "^1.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/i18next": "^12.1.0",
    "@types/i18next-browser-languagedetector": "^2.0.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.5",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.5",
    "@types/redux-actions": "^2.3.2",
    "@types/redux-mock-store": "^1.0.0",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.5",
    "history": "^4.7.2",
    "i18next": "=15.0.7",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "pkg": "^4.3.5",
    "react-i18next": "=10.6.1",
    "react-openfin-mat-impl": "^0.70.40",
    "react-redux": "^6.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "shelljs": "^0.8.3",
    "shortid": "^2.2.13",
    "typescript": "^3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-openfin": "node scripts/start.openfin.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx,mjs}",
      "!src/**/*.d.ts",
      "!src/serviceWorker.js",
      "!src/i18n.ts",
      "!src/index.tsx"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/setupTests.js",
      "<rootDir>/config/jest/jestMockUps.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.{ts,tsx,js,jsx,mjs}",
      "**/?(*.)(spec|test).{ts,tsx,js,jsx,mjs}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "globals": {},
    "testEnvironment": "jest-environment-jsdom-global",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.(ts|tsx|js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "pkg": {
    "scripts": [
      "scripts/**/*.js",
      "config/**/*.js"
    ],
    "assets": [
      "build/**/*",
      "package.json"
    ]
  }
}
